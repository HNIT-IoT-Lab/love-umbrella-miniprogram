.header {
  margin: 90rpx 0 90rpx 50rpx;
  border-bottom: 1px solid #ccc;
  text-align: center;
  width: 650rpx;
  height: 300rpx;
  line-height: 450rpx;
}

.header image {
  width: 200rpx;
  height: 200rpx;
}

.content {
  margin-left: 50rpx;
  margin-bottom: 90rpx;
}

.content text {
  display: block;
  color: #9d9d9d;
  margin-top: 40rpx;
}

.bottom {
  border-radius: 80rpx;
  margin: 70rpx 50rpx;
  font-size: 35rpx;
}

.interval{
  margin-top: 45rpx;
}
.exitBtn{
  position: absolute;
  left: 50%;
  width: 184px;
  height: 40px;
  margin-left: -92px;
  text-align: center;
  font: 18px "microsoft yahei";
  font-weight: 550;
  color: #07c160;
  padding: 8px 24px;
  background-color: #f8f8f8;
  border-radius: 50px;
}