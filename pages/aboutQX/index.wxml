<!--index.wxml-->
<view>
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for-items="{{swiper_list}}" wx:key="*this">
      <swiper-item>
        <image src="{{item.url}}" mode="widthFix" />
      </swiper-item>
    </block>
  </swiper>
  <view class='floortitle'>
    <view class='floortitlemoreicon myfont01'>计算机青协志愿活动</view>
    <view class='floortitlemoretext'>更多特色活动> </view>
  </view>
  <view class='hotgoodcontainer'>
    <view class='gooditem' 
    wx:for="{{content_list}}"
    wx:key="*this"
    bindtap="toContent" 
    data-item="{{item}}"
    >
      <view class='imgview'>
        <image class='productimg' src='{{item.url}}' />
      </view>
      <view>{{item.title}}</view>
    </view>
  </view>
  <view>
  </view>
</view>
<view class="bottom">
  <view class="foot">
    如果你也想参加我们计算机青协的活动
  </view>
  <view class="foot">你可以志愿活动页面中报名参加</view>
  <view class="foot">期待和你一起参加志愿活动</view>
  <!-- 底部版权声明 -->
  <view class="footer">
        <view class="footerImage">
            <image src="../../images/qxLogo.png" role="img"></image>
        </view>
        <view class="footerTitle">予人玫瑰，手有余香，做一名快乐的志愿者</view>
    </view>
</view>